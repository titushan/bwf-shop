<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bwf.shop.zuul.mapper.UserMapper">

    <select id="getUserByToken" resultType="com.bwf.shop.zuul.bean.po.User">
        select * from `user`
            where user_token = #{token}

    </select>


</mapper>